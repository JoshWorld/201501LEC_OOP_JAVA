import java.io.*;

public class InputExample {
	public static void main(String[] argv) {
		InputStreamReader reader = new InputStreamReader(System.in);
		
		try {
			while(true) {
				char a = (char) reader.read();
				System.out.println(a);
			}
		}
		catch (IOException e) {
			System.out.println("IO Exception");
		}
		
	}
	
}
